<div class="col-md-12">
  <h1>New Tax Record</h1>
</div>

<div class="col-md-12">
  <%= render 'shared/error_messages', :target => @tax %>
</div>

<%= form_for @tax do |f| %>
  <%= render 'taxes/fields', f: f %>

  <div class="col-md-12" >
    <div class="form_group" style="display: inline-block;">
      <%= f.submit 'Create', class: 'btn btn-primary' %>
    </div>
  </div>
<% end %>
